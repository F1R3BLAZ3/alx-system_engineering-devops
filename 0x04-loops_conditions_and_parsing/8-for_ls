#!/usr/bin/env bash
# This script displays the content of the current directory in a list format,
# showing only the part of the name after the first dash (hyphen).

for file in *; do
    if [ -f "$file" ] && [[ "$file" != .* ]]; then
        name=$(echo "$file" | awk -F'-' 'NF>1{print $2}') # Extract the part after the first dash
        if [ -n "$name" ]; then
            echo "$name"
        fi
    fi
done
